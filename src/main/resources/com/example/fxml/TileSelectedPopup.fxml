<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="206.0" prefWidth="407.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ViewController.popupController.TileSelectedController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="civilization selected" />
      <ImageView fx:id="img" fitHeight="150.0" fitWidth="200.0" layoutX="243.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/pic/civilization.png" />
         </image>
      </ImageView>
      <Button fx:id="cheat_btn" layoutX="45.0" layoutY="60.0" mnemonicParsing="false"
              onMouseClicked="#cheat" prefHeight="25.0" prefWidth="159.0" text="cheat"/>
      <Button fx:id="research_btn" layoutX="45.0" layoutY="95.0" mnemonicParsing="false" onMouseClicked="#research" prefHeight="25.0" prefWidth="159.0" text="researach" />
      <Button fx:id="endturn_btn" layoutX="45.0" layoutY="129.0" mnemonicParsing="false" onMouseClicked="#endTurn" prefHeight="25.0" prefWidth="159.0" text="end turn" />
      <Button fx:id="back_btn" layoutX="10.0" layoutY="171.0" mnemonicParsing="false" onMouseClicked="#back" prefHeight="25.0" prefWidth="30.0" text="&lt;" />
   </children>
</AnchorPane>
